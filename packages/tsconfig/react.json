{
  "$schema": "https://json.schemastore.org/tsconfig.json",
  "display": "React.js",
  "extends": "./base.json",

  "compilerOptions": {
    "useDefineForClassFields": true,

    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "noEmit": true,
    "jsx": "react-jsx",

    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true
  }
}
